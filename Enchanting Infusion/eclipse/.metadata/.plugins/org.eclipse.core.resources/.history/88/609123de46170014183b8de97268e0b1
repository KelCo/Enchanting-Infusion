package com.limitlessPotential.enchantingInfusion.enchantments;

import net.minecraft.enchantment.Enchantment;
import net.minecraft.enchantment.EnumEnchantmentType;

public class EnchantmentJump extends Enchantment{

	public EnchantmentJump(int id, int rarity) {
		super(id, rarity, EnumEnchantmentType.armor_feet);
		this.setName("jump");
	
	
	
	
	}
	
	public int getMinEnchantability(int par1) {
		return 20 + (par1 - 1) * 10;
	}
	
	
	public int getMaxEnchantablility(int par1) {
		return this.getMinEnchantability(par1) + 10;
	
	}
	
	public int getMaxLevel() {
		return 3;
	}
	
	
	
	
}
